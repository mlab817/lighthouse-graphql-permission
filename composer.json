{
    "name": "mlab817/lighthouse-graphql-permission",
    "description": "Add GraphQL types and mutations for spatie/laravel-permission",
    "type": "library",
    "keywords": ["laravel","lighthouse","GraphQL","spatie","laravel-permission"],
    "require": {
        "php": "^7.3|^8.0",
        "spatie/laravel-permission": "^3.18",
        "nuwave/lighthouse": "^4.18"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Lester A. Bolotaolo",
            "email": "mlab817@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mlab817\\LighthouseGraphQLPermission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mlab817\\LighthouseGraphQLPermission\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Mlab817\\LighthouseGraphQLPermission\\Providers\\LighthouseGraphQLPermissionServiceProvider"
            ]
        }
    }
}
